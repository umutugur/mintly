#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules/@expo/fingerprint/bin/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules/@expo/fingerprint/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules/@expo/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules/@expo/fingerprint/bin/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules/@expo/fingerprint/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules/@expo/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@expo+fingerprint@0.15.4/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
