#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules/@sentry/react-native/scripts/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules/@sentry/react-native/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules/@sentry/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules/@sentry/react-native/scripts/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules/@sentry/react-native/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules/@sentry/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/@sentry+react-native@7.2.0_expo@54.0.33_@babel+core@7.29.0_react-native@0.81.5_@babel+c_4ec525af8334dd58c2ee89afe3d28cfc/node_modules:/Users/umutugur/finsight/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sentry/react-native/scripts/expo-upload-sourcemaps.js" "$@"
else
  exec node  "$basedir/../@sentry/react-native/scripts/expo-upload-sourcemaps.js" "$@"
fi
