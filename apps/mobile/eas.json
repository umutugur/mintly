{
  "cli": {
    "version": ">= 16.20.4",
    "appVersionSource": "local"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "environment": "preview",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "profile": {
      "distribution": "internal",
      "environment": "preview",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}
